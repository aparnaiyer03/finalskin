<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.tflitecamerademo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MaterialTheme"
        tools:replace="android:theme,android:icon">
        <activity android:name=".healthy"
            android:theme="@style/Base.Theme.AppCompat"></activity>
        <activity android:name=".pigmentation"
            android:theme="@style/Base.Theme.AppCompat"/>
        <activity android:name=".rosacea"
            android:theme="@style/Base.Theme.AppCompat"/>
        <activity android:name=".acne"
            android:theme="@style/Base.Theme.AppCompat"/>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".SettingsActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".SearchProductsActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".ProductDetailsActivity"
            android:theme="@style/Base.Theme.AppCompat"/>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme1.NoActionBar" />
        <activity android:name=".ConfirmFinalOrderActivity"
            android:theme="@style/AppTheme1.NoActionBar"/>
        <activity
            android:name=".AdminUserProductsActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".CartActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".AdminNewOrdersActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".AdminCategoryActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".AdminAddNewProductActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".CameraActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demo"
            android:label="@string/app_name"
            android:parentActivityName=".demo">
            <intent-filter>
                <action android:name="com.example.android.tflitecamerademo.demo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demo2"
            android:label="Demo2"
            />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>